{
  "name": "greenpoint",
  "version": "1.0.0",
  "description": "An app to challenge users to be more environmentally conscious in their actions.",
  "main": "server.js",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "react-chartjs-2": "^2.8.0",
    "react-collapsible": "^2.6.0",
    "react-datepicker": "^2.9.6",
    "react-toastify": "^5.4.0",
    "socket.io": "^2.3.0",
    "twilio": "^3.36.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.2",
    "readme-jsx": "^0.3.0"
  },
  "scripts": {
    "readme": "node ./scripts/readme-gen.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brityank/greenpoint.git"
  },
  "keywords": [
    "conserve",
    "green",
    "environment",
    "tree",
    "recycle"
  ],
  "contributors": [
    "brityank",
    "kevdsteele",
    "kimSpiegel04",
    "javierjpagan",
    "DavidPomarede"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brityank/greenpoint/issues"
  },
  "homepage": "https://github.com/brityank/greenpoint#readme"
}
